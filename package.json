{
  "name": "@broadpeak/simid",
  "version": "2.0.0",
  "description": "SIMID client",
  "author": "Broadpeak",
  "typings": "types.d.ts",
  "main": "dist/simid.js",
  "type": "module",
  "files": [],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "dev": "rollup --watch -c rollup.config.js",
    "start": "node server.cjs",
    "webos:package": "cd tvapps/webos && ares-package .",
    "webos:install": "cd tvapps/webos && ares-install tv.broadpeak.demo_0.0.1_all.ipk",
    "webos:run": "cd tvapps/webos && ares-launch tv.broadpeak.demo_0.0.1_all.ipk",
    "webos:debug": "cd tvapps/webos && ares-inspect --app tv.broadpeak.demo --open"
  },
  "dependencies": {
    "eventemitter3": "5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.14.6",
    "express": "4.18.2",
    "mkdirp": "^3.0.1",
    "node-fetch": "3.3.1",
    "rollup": "^2.79.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-deepmerge": "^6.2.0",
    "typescript": "^4.9.5"
  }
}
